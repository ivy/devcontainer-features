{
  "with_username": {
      "image": "mcr.microsoft.com/devcontainers/base:debian",
      "features": {
          "ghcr.io/devcontainers/features/common-utils:1": {
              "username": "octocat"
          },
          "aider": {}
      },
      "remoteUser": "octocat"
  },
  "with_version": {
      "image": "mcr.microsoft.com/devcontainers/base:debian",
      "features": {
          "aider": {
              "version": "0.69.0"
          }
      }
  },
  "without_playwright": {
      "image": "mcr.microsoft.com/devcontainers/base:debian",
      "features": {
          "aider": {
              "installPlaywright": false
          }
      }
  },
  "with_playwright_browsers": {
      "image": "mcr.microsoft.com/devcontainers/base:debian",
      "features": {
          "aider": {
              "installPlaywrightBrowsers": "chromium,firefox"
          }
      }
  }
}
